<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:background="#B3B5E7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Result_korpus">
    <RelativeLayout
        android:id="@+id/RL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/logo"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.049"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />
        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/fil"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="15dp"
            android:layout_toRightOf="@+id/logo"
            android:layout_marginStart="225dp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RL11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/RL2"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:gravity="center">

        <TextView
            android:id="@+id/nameTests"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/lora"
            android:gravity="center"
            android:text="Результаты по корпусу"
            android:textColor="#606287"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </RelativeLayout>


    <ListView
        android:id="@+id/r"
        android:layout_width="wrap_content"
        android:layout_height="488dp"
        android:layout_below="@id/RL1"
        android:layout_gravity="center"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="10dp"
        android:gravity="center" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/Sv"
        android:layout_gravity="center"
        android:layout_marginTop="35dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Calm"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginStart="160dp"
            android:onClick="corp"
            android:src="@drawable/housing" />


    </RelativeLayout>
</LinearLayout>